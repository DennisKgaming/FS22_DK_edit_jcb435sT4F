<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<sounds xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../shared/xml/schema/vehicle_sounds.xsd">
    <motorized>
      <sounds>
        <motorStart template="DEFAULT_MOTOR_START" file="sounds/jcb_435s_start.wav" linkNode="0>" volumeScale="4"/>
        <motorStop template="DEFAULT_MOTOR_STOP" file="sounds/jcb_435s_stop.wav" linkNode="0>" volumeScale="4"/>
    
        <motor template="DEFAULT_MOTOR_IDLE" file="sounds/jcb_435s_idle_loop.wav" linkNode="0>" volumeScale="4"/>
        <motor template="DEFAULT_MOTOR_RUN" file="sounds/jcb_435s_run_loop.wav" linkNode="0>" volumeScale="4"/>
    
        <gearbox template="transmissionLarge" file="$data/sounds/vehicles/transmission/transmissionLoop.wav" pitchScale="0.8" volumeScale="0.5"/>
        <retarder template="DEFAULT_RETARDER" file="$data/sounds/vehicles/retarder/retarder_loop_01.wav" linkNode="0>" volumeScale="2"/>
    
        <reverseDrive template="REVERSE_BEEP_03" linkNode="0>" />
		
		<motor template="filterLoop" fadeIn="3" fadeOut="2" volumeScale="0.55" pitchScale="0.8" linkNode="airFilterCleaner"/>
		<compressedAir template="compressedAir"/>
        <airCompressorRun template="brakeCompressorLarge" />
        <airCompressorStop template="compressedAir"/>
		<motor template="indoorCabinRumble" />
      </sounds>
    </motorized>

    <lights>
        <sounds>
            <toggleLights template="SWITCH_01"  />
            <turnLight template="SWITCH_TURNLIGHT"  />
        </sounds>
    </lights>

    <attacherJoints>
        <sounds>
            <attach template="ATTACH_01" />
            <hydraulic template="defaultHydraulicSound" />
        </sounds>
    </attacherJoints>

    <drivable>
        <sounds>
            <waterSplash template="WATER_SPLASH_01" />
        </sounds>
    </drivable>

    <honk>
        <sound template="HONK_CLAAS_XERION" />
    </honk>
</sounds>
